{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  //"rules": {}
  "rules": {
    //使われていない変数を無視
    "varsIgnorePattern": "off",
    //プロパティのバリデーションチェックを無視
    "react/prop-types": "off"
  }
  //"rules": {"react/prop-types": "off"}
}
